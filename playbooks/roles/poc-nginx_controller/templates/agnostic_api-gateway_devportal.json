{
  "metadata": {
    "name": "gw_dev_{{extra_app.name | lower}}",
    "displayName": "Gateways DevPortal {{extra_app.name | lower}}",
    "description": "Generated by CMP"
  },
  "desiredState": {
    "ingress": {
      "tls": {
        "certRef": {
          "ref": "/services/environments/{{ var_env_prefix }}{{ extra_app.environment | lower }}/certs/crt_dev_{{ extra_app.name | lower }}"
        }
      },
      "uris": {
        "https://dev-{{extra_app.name | lower}}.{{extra_app.domain | lower}}": {
          "matchMethod": "EXACT"
        }
      },
      "placement": {
        "instanceRefs": [
          {
            "ref": "/infrastructure/locations/devportal/instances/{{ extra_devportal.vm_name }}"
          }
        ]
      }
    }
  }
}
